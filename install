echo "Options:"
echo "1. Install dionaea honeypot"
echo "2. Install dashboard"
echo "3. Run dionaea honeypot"
echo "4. Run dashboard"
echo "5. Exit"

while true; do
	read -p "Choose an option:[1-5] " option
	case $option in
		[1]* ) echo "Install dionaea";;
		[2]* ) echo "Install dashboard";;
		[3]* ) echo "Run dionaea";;
		[4]* ) echo "Run dashboard";;
		[5]* ) echo "Bye Bye"; break;;
		* ) echo "This is not an option, please choose one of these"
			echo "1. Install dionaea honeypot"
			echo "2. Install dashboard"
			echo "3. Run dionaea honeypot"
			echo "4. Run dashboard"
			echo "5. Exit";;
	esac
done
