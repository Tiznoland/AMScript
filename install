echo "DDDD    DDDD  RRRR   RRRR    YYYY  YYYY       BBBB   BBBB  EEEE       LLLL         TTTT"
echo "DDDD     DDDD RRRR  RRRR      YYYYYYYY        BBBBBBBBBB   EEEEEE     LLLL         TTTT"
echo "DDDD    DDDD  RRRRRRRRR        YYYYYY         BBBB   BBBB  EEEE       LLLL         TTTT"
echo "DDDD   DDDD   RRRRRRRR          YYYY          BBBB  BBBB   EEEE       LLLL         TTTT"
